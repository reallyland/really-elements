{
  "name": "@reallyland/really-elements",
  "version": "0.8.0",
  "description": "A collection of opinionated custom elements for the web",
  "keywords": [
    "elements",
    "lit-element",
    "lit-html",
    "polymer",
    "really",
    "really-elements",
    "typescript",
    "web-components"
  ],
  "homepage": "https://github.com/reallyland/really-elements",
  "repository": {
    "type": "git",
    "url": "git@github.com:reallyland/really-elements.git"
  },
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng (motss)",
    "email": "wes.ngrongsen@gmail.com",
    "url": "https://github.com/motss"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./clipboard-copy": "./dist/clipboard-copy/clipboard-copy.js",
    "./code-configurator": "./dist/code-configurator/code-configurator.js"
  },
  "files": [
    "dist/**/*.d.ts.map",
    "dist/**/*.d.ts",
    "dist/**/*.js.map",
    "dist/**/*.js"
  ],
  "scripts": {
    "check": "package-check",
    "build": "tsc -p tsconfig.prod.json",
    "clean": "shx rm -rf dist/",
    "install:deps": "sh ./src/scripts/npm-install.sh",
    "postinstall": "npm dedupe",
    "lint": "eslint src/**/*.ts --ext .js,.jsx,.ts,.tsx",
    "lint:build": "npm run lint -- --config .build.eslintrc.json",
    "prepublishOnly": "npm run clean && npm run build",
    "serve": "es-dev-server --watch --port=3000 --node-resolve --dedupe",
    "test": "npm run lint && wct --skip-selenium-install --config-file wct.config.json",
    "test:ci": "./node_modules/.bin/wct --config-file ./wct.config.ci.json",
    "test:sl": "./node_modules/.bin/wct --config-file ./wct.config.sl.json",
    "ts": "tsc",
    "watch": "tsc --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check && npm run lint && tsc --noEmit"
    }
  },
  "dependencies": {
    "@material/mwc-button": "^0.20.0",
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1",
    "nodemod": "^2.6.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@reallyland/tsconfig": "^2.0.0",
    "@skypack/package-check": "^0.2.2",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^8.2.0",
    "@types/prismjs": "^1.16.0",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "chai": "^4.2.0",
    "es-dev-server": "^2.0.2",
    "eslint": "^7.16.0",
    "eslint-plugin-lit": "^1.3.0",
    "eslint-plugin-lit-a11y": "^1.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-regexp": "^0.1.1",
    "esm": "^3.2.25",
    "husky": "^4.2.5",
    "mocha": "^7.2.0",
    "shx": "^0.3.2",
    "typescript": "^3.9.7",
    "wct-mocha": "^1.1.0"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
