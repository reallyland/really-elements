{
  "name": "@reallyland/really-elements",
  "version": "0.10.8-rc.7",
  "description": "A collection of opinionated custom elements for the web",
  "keywords": [
    "elements",
    "lit",
    "lit-element",
    "lit-html",
    "polymer",
    "really",
    "really-elements",
    "typescript",
    "web-components"
  ],
  "homepage": "https://github.com/reallyland/really-elements",
  "repository": {
    "type": "git",
    "url": "git@github.com:reallyland/really-elements.git"
  },
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng (motss)",
    "email": "wes.ngrongsen@gmail.com",
    "url": "https://github.com/motss"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./clipboard-copy": "./dist/clipboard-copy/clipboard-copy.js",
    "./code-configurator": "./dist/code-configurator/code-configurator.js",
    "./dist/*": "./dist/*",
    "./really-clipboard-copy": "./dist/clipboard-copy/really-clipboard-copy.js",
    "./really-code-configurator": "./dist/code-configurator/really-code-configurator.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/**/*.d.ts.map",
    "dist/**/*.d.ts",
    "dist/**/*.js.map",
    "dist/**/*.js",
    "!**/*demo*/**/*.*",
    "!**/*test*/**/*.*"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.prod.json",
    "clean": "sh ./node_modules/@reallyland/tools/cleanup.sh",
    "dev": "npm run serve & npm run watch",
    "postinstall": "sh ./node_modules/@reallyland/tools/postinstall.sh",
    "lint": "sh ./node_modules/@reallyland/tools/lint-build.sh -c ./node_modules/@reallyland/tools/browser/lit/.eslintrc.json",
    "lint-commit": "sh ./node_modules/@reallyland/tools/lint-commit.sh",
    "lint:build": "sh ./node_modules/@reallyland/tools/lint-build.sh -c ./node_modules/@reallyland/tools/browser/lit/.build.eslintrc.json",
    "pre-commit": "sh ./node_modules/@reallyland/tools/pre-commit.sh",
    "prepublishOnly": "npm run lint:build && npm run build",
    "serve": "npm x -y @web/dev-server@latest -- wds --node-resolve -dw -p 3000",
    "test": "npm run wtr",
    "watch": "npm run clean && tsc --watch",
    "wtr": "node --max-old-space-size=8192 --trace-deprecation node_modules/.bin/wtr --config wtr.config.mjs"
  },
  "dependencies": {
    "@material/mwc-button": "^0.25.3",
    "@reallyland/esm": "^0.0.4",
    "lit": "^2.2.1",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@open-wc/testing": "^3.1.2",
    "@reallyland/tools": "^0.0.1-rc.17",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/prismjs": "^1.26.0",
    "@web/dev-server-esbuild": "^0.2.10",
    "@web/test-runner": "^0.13.27",
    "@web/test-runner-mocha": "^0.7.2",
    "@web/test-runner-playwright": "^0.8.4",
    "eslint-plugin-lit": "^1.6.1",
    "eslint-plugin-lit-a11y": "^2.2.0"
  },
  "engines": {
    "node": ">= 16.x",
    "npm": ">= 8.x"
  },
  "publishConfig": {
    "access": "public"
  }
}
