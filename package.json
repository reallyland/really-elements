{
  "name": "@reallyland/really-elements",
  "version": "0.3.0",
  "description": "A collection of opinionated custom elements for the web",
  "keywords": [
    "elements",
    "lit-element",
    "lit-html",
    "polymer",
    "really",
    "really-elements",
    "typescript",
    "web-components"
  ],
  "homepage": "https://github.com/reallyland/really-elements",
  "repository": {
    "type": "git",
    "url": "git@github.com:reallyland/really-elements.git"
  },
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng (motss)",
    "email": "wes.ngrongsen@gmail.com",
    "url": "https://github.com/motss"
  },
  "files": [
    "dist/**/*.d.ts.map",
    "dist/**/*.d.ts",
    "dist/**/*.js.map",
    "dist/**/*.js"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "scripts": {
    "build": "npm run ts -- -p tsconfig.prod.json",
    "clean": "shx rm -rf dist/",
    "gulp": "gulp",
    "install:deps": "sh ./src/scripts/npm-install.sh",
    "lint": "tslint --project tsconfig.json --config tslint.prod.json --format stylish",
    "lint:debug": "tslint --project tsconfig.json --config tslint.json --format stylish",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "npm run lint && wct --skip-selenium-install --config-file wct.config.json",
    "test:ci": "wct --config-file ./wct.config.ci.json",
    "test:sl": "wct --config-file ./wct.config.sl.json",
    "ts": "tsc",
    "watch": "npm run ts -- --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run ts && npm run clean"
    }
  },
  "dependencies": {
    "@material/mwc-button": "^0.11.1",
    "lit-element": "^2.2.1",
    "lit-html": "^1.1.2",
    "nodemod": "^2.2.3",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@reallyland/tsconfig": "^1.0.1",
    "@reallyland/tslint-config": "^1.1.1",
    "@types/chai": "^4.2.6",
    "@types/mocha": "^5.2.7",
    "@types/prismjs": "^1.16.0",
    "chai": "^4.2.0",
    "esm": "^3.2.25",
    "husky": "^3.1.0",
    "mocha": "^6.2.2",
    "shx": "^0.3.2",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3",
    "wct-mocha": "^1.0.1"
  },
  "engines": {
    "node": ">= 8.16.0",
    "npm": ">= 6.4.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
