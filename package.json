{
  "name": "@reallyland/really-elements",
  "version": "0.8.0",
  "description": "A collection of opinionated custom elements for the web",
  "keywords": [
    "elements",
    "lit-element",
    "lit-html",
    "polymer",
    "really",
    "really-elements",
    "typescript",
    "web-components"
  ],
  "homepage": "https://github.com/reallyland/really-elements",
  "repository": {
    "type": "git",
    "url": "git@github.com:reallyland/really-elements.git"
  },
  "license": "MIT",
  "author": {
    "name": "Rong Sen Ng (motss)",
    "email": "wes.ngrongsen@gmail.com",
    "url": "https://github.com/motss"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./clipboard-copy": "./dist/clipboard-copy/clipboard-copy.js",
    "./code-configurator": "./dist/code-configurator/code-configurator.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist/**/*.d.ts.map",
    "dist/**/*.d.ts",
    "dist/**/*.js.map",
    "dist/**/*.js"
  ],
  "scripts": {
    "//install:deps": "sh ./src/scripts/npm-install.sh",
    "//test": "npm run lint && wct --skip-selenium-install --config-file wct.config.json",
    "//test:ci": "./node_modules/.bin/wct --config-file ./wct.config.ci.json",
    "//test:sl": "./node_modules/.bin/wct --config-file ./wct.config.sl.json",
    "build": "tsc -p tsconfig.prod.json",
    "check": "package-check",
    "clean": "shx rm -rf .*cache *.log coverage/ dist/ logs/",
    "postinstall": "npm dedupe",
    "lint": "tslint --project tsconfig.json --config tslint.prod.json --format stylish",
    "lint:debug": "tslint --project tsconfig.json --config tslint.json --format stylish",
    "prepublishOnly": "npm run lint && npm run build",
    "test": "wtr",
    "ts": "tsc",
    "watch": "wtr --watch",
    "wtr": "node --max-old-space-size=8192 --trace-deprecation node_modules/@web/test-runner/dist/bin.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run check && npm run lint && tsc --noEmit"
    }
  },
  "dependencies": {
    "@material/mwc-button": "^0.20.0",
    "lit-element": "^2.3.1",
    "lit-html": "^1.2.1",
    "nodemod": "^2.6.1",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@esm-bundle/chai": "^4.1.5",
    "@open-wc/testing": "^2.5.32",
    "@reallyland/tsconfig": "^2.0.0",
    "@reallyland/tslint-config": "^1.1.1",
    "@skypack/package-check": "^0.2.2",
    "@types/prismjs": "^1.16.0",
    "@web/dev-server-esbuild": "^0.2.10",
    "@web/test-runner": "^0.11.7",
    "@web/test-runner-mocha": "^0.6.0",
    "@web/test-runner-playwright": "^0.7.2",
    "husky": "^4.2.5",
    "mocha": "^8.2.1",
    "tslint": "^5.20.1",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">= 10.18.1",
    "npm": ">= 6.13.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
